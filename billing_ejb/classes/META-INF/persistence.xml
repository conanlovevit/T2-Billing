<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">
  <persistence-unit name="billing_ejb">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:/app/jdbc/jdbc/ztvt2_billingDS</jta-data-source>
    <class>com.viettel.billing.bl.database.TblAccount</class>
    <class>com.viettel.billing.bl.database.TblApp</class>
    <class>com.viettel.billing.bl.database.TblAppItem</class>
    <class>com.viettel.billing.bl.database.TblAppVersion</class>
    <class>com.viettel.billing.bl.database.TblConfig</class>
    <class>com.viettel.billing.bl.database.TblDeviceModel</class>
    <class>com.viettel.billing.bl.database.TblDeviceOs</class>
    <class>com.viettel.billing.bl.database.TblDeviceType</class>
    <class>com.viettel.billing.bl.database.TblLocation</class>
    <class>com.viettel.billing.bl.database.TblDevice</class>
    <class>com.viettel.billing.bl.database.TblPackage</class>
    <properties>
      <property name="javax.persistence.jtaDataSource" value="java:/app/jdbc/jdbc/ztvt2_billingDS"/>
    </properties>
  </persistence-unit>
</persistence>
